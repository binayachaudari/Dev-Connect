{
  "name": "dev-connect",
  "version": "1.0.0",
  "description": "Social Network for Developers",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm start --prefix client",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
    "prettier": "prettier --single-quote \"{client,api}/**/*.js\" \"{client,api}/**/*.css\"",
    "dev": "concurrently \" npm run server \" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binayachaudari/Dev-Connect.git"
  },
  "keywords": [
    "MERN",
    "stack",
    "dev-connect"
  ],
  "author": "Binaya Kumar Chaudhary",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/binayachaudari/Dev-Connect/issues"
  },
  "homepage": "https://github.com/binayachaudari/Dev-Connect#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "config": "^3.2.2",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.7.5",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "prettier": "^2.0.5",
    "redux-devtools-extension": "^2.13.8"
  }
}
